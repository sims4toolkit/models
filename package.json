{
  "name": "@s4tk/models",
  "version": "0.4.2",
  "description": "Models for reading and writing package files and the resources they contain.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sims4toolkit/models.git"
  },
  "homepage": "https://sims4toolkit.com/#/docs/models",
  "bugs": {
    "url": "https://github.com/sims4toolkit/models/issues"
  },
  "main": "./models.js",
  "scripts": {
    "clean": "rm -rf ./dst/**",
    "dev": "npm run clean ; npm run build:ffi; tsc --watch",
    "dev:serve": "npm run build && ts-node \"./server/app.ts\"",
    "build": "npm run clean ; npm run build:ffi ; tsc ; node scripts/prepublish.cjs",
    "build:ffi": "cp -R ./src/ffi ./dst/ffi",
    "publish": "npm run build && sh scripts/publish.sh",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\"",
    "test:coverage": "nyc mocha -r ts-node/register \"test/**/*.test.ts\"",
    "test:current": "mocha -r ts-node/register \"test/**/package.test.ts\""
  },
  "author": "frankkulak",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/bePatron?u=40823163"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "body-parser": "^1.19.1",
    "chai": "^4.3.4",
    "express": "^4.17.2",
    "glob": "^7.2.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@s4tk/compression": "^0.2.0",
    "@s4tk/encoding": "^0.1.2",
    "@s4tk/hashing": "^0.2.0",
    "@s4tk/xml-dom": "^0.1.3",
    "just-clone": "^5.0.1",
    "just-compare": "^2.0.1"
  }
}