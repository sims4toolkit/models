      "prefix": "class",
      "name": "Package",
      "generics": [
        {
          "name": "ResourceType",
          "extends": [
            {
              "name": "Resource",
              "path": {
                "pkg": "models",
                "group": "types",
                "item": "Resource"
              }
            }
          ]
        }
      ],
      "extends": [
        {
          "name": "MappedModel<ResourceKey, Resource, ResourceEntry<ResourceType>>",
