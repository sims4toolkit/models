    {
      "title": "Functions",
      "description": "Helper functions related to SimData groups.",
      "content": [
        {
          "displayType": "function",
          "prefix": "static",
          "name": "all",
          "description": "Returns an array of all groups in the enum.",
          "returnType": {
            "name": "SimDataGroup[]"
          }
        },
        {
          "displayType": "function",
          "prefix": "static",
          "name": "getForTuning",
          "description": "Returns the SimDataGroup to use for the given tuning type. The given value does not have to be from the enum, it can also be the plain number. Note that not all tuning types have an assiciated SimDataGroup (for example, streets use the Tuning type, but the Street group).",
          "returnType": {
            "name": "SimDataGroup"
          },
          "arguments": [
            {
              "name": "tuningType",
              "description": "Tuning type to get SimData group for.",
              "type": {
                "name": "TuningResourceType",
                "path": {
                  "pkg": "models",
                  "group": "enums",
                  "item": "TuningResourceType"
                }
              }
            }
          ],
          "examples": [
            {
              "code": "const group = SimDataGroup.getForTuning(TuningResourceType.Trait);"
            }
          ]
        }
      ]
